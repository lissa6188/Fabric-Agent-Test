{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Azure.AI.OpenAI": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureAI": {
    "ProjectEndpoint": "https://YOUR-RESOURCE-NAME.openai.azure.com/",
    "DeploymentName": "gpt-4o",
    "ApiKey": "YOUR_API_KEY_HERE"
  },
  "Fabric": {
    "ConnectionString": "YOUR_FABRIC_CONNECTION_STRING",
    "WorkspaceId": "YOUR_WORKSPACE_ID",
    "LakehouseName": "YOUR_LAKEHOUSE_NAME"
  },
  "SemanticKernel": {
    "MaxTokens": 4000,
    "Temperature": 0.7,
    "EnableMemory": true
  }
}
